# Naturinex Client Environment Variables
# Copy this file to .env and fill in your actual values

# API Configuration
REACT_APP_API_URL=https://naturinex-app.onrender.com

# Stripe Configuration
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here

# Firebase Configuration
REACT_APP_FIREBASE_API_KEY=your_firebase_api_key_here
REACT_APP_FIREBASE_AUTH_DOMAIN=your_project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your_project_id
REACT_APP_FIREBASE_STORAGE_BUCKET=your_project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your_sender_id
REACT_APP_FIREBASE_APP_ID=your_app_id

# Google OAuth Configuration
REACT_APP_GOOGLE_EXPO_CLIENT_ID=your_google_expo_client_id
REACT_APP_GOOGLE_IOS_CLIENT_ID=your_google_ios_client_id
REACT_APP_GOOGLE_ANDROID_CLIENT_ID=your_google_android_client_id

# App Configuration
REACT_APP_ENV=production
REACT_APP_VERSION=1.0.0 