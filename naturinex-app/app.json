{
  "expo": {
    "name": "Naturinex Wellness Guide",
    "slug": "naturinex",
    "owner": "guampaul",
    "version": "1.0.0",
    "sdkVersion": "52.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "orientation": "portrait",
    "icon": "./assets/icon-1024.png",
    "userInterfaceStyle": "light",
    "scheme": "naturinex",
    "splash": {
      "image": "./assets/naturinex-splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#10B981"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.naturinex.app",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Naturinex Wellness Guide uses the camera to capture text from product labels for educational wellness information lookup",
        "NSPhotoLibraryUsageDescription": "Naturinex Wellness Guide accesses photos to analyze product labels and provide educational wellness information",
        "NSPhotoLibraryAddUsageDescription": "Naturinex Wellness Guide can save educational wellness information to your photo library for future reference"
      },
      "config": {
        "usesNonExemptEncryption": false
      },
      "buildNumber": "2"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-512.png",
        "backgroundColor": "#10B981"
      },
      "package": "com.naturinex.app",
      "permissions": [
        "CAMERA",
        "READ_EXTERNAL_STORAGE"
      ],
      "versionCode": 1
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "ios": {
            "deploymentTarget": "15.1",
            "useFrameworks": "static"
          }
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Naturinex Wellness Guide uses the camera to capture product labels for educational wellness information"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Naturinex needs access to your photos to analyze medication images",
