{
  "version": 2,
  "name": "naturinex-app",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/server/index.js"
    },
    {
      "source": "/suggest",
      "destination": "/server/index.js"
    },
    {
      "source": "/webhook",
      "destination": "/server/index.js"
    },
    {
      "source": "/webhooks/(.*)",
      "destination": "/server/index.js"
    },
    {
      "source": "/health",
      "destination": "/server/index.js"
    }
  ],
  "functions": {
    "server/index.js": {
      "maxDuration": 30
    }
  },
  "buildCommand": "cd naturinex-app && npm install && npm run build:web",
  "outputDirectory": "naturinex-app/build",
  "installCommand": "npm install && cd server && npm install && cd ../naturinex-app && npm install",
  "framework": null
}